{
  "name": "ssr-react-router-node-app",
  "version": "1.0.0",
  "description": "SSR React Route Node App",
  "main": "index.js",
  "scripts": {
    "build": "babel ./app -d build",
    "build:watch": "babel ./app -d build --watch",
    "build:client": "webpack --config ./webpack.config.js/",
    "build:prod": "npm run build && npm run build:client",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "start": "npm run build:prod && NODE_ENV=production node ./build/server/index.js",
    "start:dev": "parallelshell 'npm run build:watch' 'npm run build:watch:client' 'nodemon ./build/server/index.js'",
    "test": "jest --watch --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "axios": "^0.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-upgrade": "0.0.23",
    "bootstrap": "^4.1.3",
    "bundle-loader": "^0.5.6",
    "cors": "^2.8.4",
    "enzyme": "^3.7.0",
    "express": "^4.16.4",
    "fsevents": "^1.2.4",
    "jest": "^23.6.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "supertest": "^3.3.0",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.5",
    "parallelshell": "^3.0.2",
    "webpack": "^4.24.0"
  }
}
